{
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  },
  "environments": {
    "production": {
      "variables": {
        "PYTHONPATH": "/app",
        "PYTHONUNBUFFERED": "1"
      }
    }
  }
}